<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.wwinter.malladmin.mapper.CmsPreferenceAreaProductRelationMapper">

    <resultMap id="cmsPreferenceAreaProductRelationResultMap"
               type="cn.wwinter.malladmin.model.domain.CmsPreferenceAreaProductRelation">
        <id column="id" property="id" jdbcType="BIGINT" javaType="Long"/>
        <result column="product_id" property="productId" jdbcType="BIGINT" javaType="Long"/>
        <result column="preference_areaId" property="preferenceAreaId" jdbcType="BIGINT" javaType="Long"/>
    </resultMap>

    <sql id="CmsPreferenceAreaProductRelationResultMapSql">
        id,
        product_id,
        preference_areaId
    </sql>

    <!--批量新增回写主键支持-->
    <insert id="insertList" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO cms_subject_product_relation (<include refid="CmsPreferenceAreaProductRelationResultMapSql"/>)
        VALUES
        <foreach collection="list" separator="," item="item" index="index">
            #{item.id},
            #{item.productId},
            #{item.preferenceAreaId}
        </foreach>
    </insert>


</mapper>